# Production Environment Variables
# These are used by Vercel during deployment

# Force the main server to run instead of fallback
FORCE_MAIN_SERVER=true

# App URL and configuration
NEXT_PUBLIC_APP_URL=https://tierd-deployment.vercel.app
NEXTAUTH_URL=https://tierd-deployment.vercel.app
NODE_ENV=production

# Force clean build
FRESH_DEPLOY=true
FORCE_CLEAN_START=true

# Vercel-specific
VERCEL=true

# Skip build-time type checking and linting to ensure deployment works
NEXT_SKIP_TYPE_CHECK=true
NEXT_TELEMETRY_DISABLED=1

# Use exact local version
USE_LOCAL_VERSION=true
PRESERVE_EXACT_BUILD=true

# Skip unnecessary optimizations
SKIP_OPTIMIZATION=true

# Skip instrumentation
EDGE_CONFIG=skip 